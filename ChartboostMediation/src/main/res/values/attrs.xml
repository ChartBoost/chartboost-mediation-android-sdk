<resources>
    <declare-styleable name="ChartboostMediationBannerAd">
        <attr name="chartboostMediationPlacement" format="string"/>
        <attr name="chartboostMediationBannerSize" format="enum">
            <enum name="STANDARD" value="0"/>
            <enum name="MEDIUM" value="1"/>
            <enum name="LEADERBOARD" value="2"/>
            <enum name="ADAPTIVE" value="3"/>
        </attr>
        <attr name="chartboostMediationBannerFlexibleWidth" format="dimension"/>
        <attr name="chartboostMediationBannerFlexibleHeight" format="dimension"/>
    </declare-styleable>
</resources>
