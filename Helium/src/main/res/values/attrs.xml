<resources>
    <declare-styleable name="HeliumBannerAd">
        <attr name="heliumPlacementName" format="string"/>
        <attr name="heliumBannerSize" format="enum">
            <enum name="STANDARD" value="0"/>
            <enum name="MEDIUM" value="1"/>
            <enum name="LEADERBOARD" value="2"/>
            <enum name="ADAPTIVE" value="3"/>
        </attr>
        <attr name="heliumBannerFlexibleWidth" format="dimension"/>
        <attr name="heliumBannerFlexibleHeight" format="dimension"/>
    </declare-styleable>
</resources>
